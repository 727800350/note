#!/bin/bash
set -x

dir=`pwd`
cd $dir

## latex
rm *.aux *.bbl *.blg *.lof *.lot *.toc *.thm *.snm *.vrb *.nav 2>/dev/null

## glog
rm *.TENCENT64.site.ericuniwang.log.* 2>/dev/null
for file in `ls *.INFO *.WARNING *.ERROR *.FATAL`
do
	if [ -L $file ]
	then
		rm $file
	fi
done 2>/dev/null

for file in `ls *.out *.log`
do
	## preserve nohup.out and err.log
	if [ "x$file" == "xnohup.out" -o "x$file" == "xerr.log" ]
	then
		continue
	fi
	rm $file
done 2>/dev/null

exit 0

